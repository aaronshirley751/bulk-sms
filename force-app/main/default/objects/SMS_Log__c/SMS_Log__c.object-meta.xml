<?xml version="1.0" encoding="UTF-8"?>
<CustomObject xmlns="http://soap.sforce.com/2006/04/metadata">
    <label>SMS Log</label>
    <pluralLabel>SMS Logs</pluralLabel>

    <!-- Auto-number primary key -->
    <nameField>
        <label>SMS Log #</label>
        <type>AutoNumber</type>
    </nameField>

    <deploymentStatus>Deployed</deploymentStatus>
    <sharingModel>ReadWrite</sharingModel>
    <enableActivities>true</enableActivities>
    <enableReports>true</enableReports>

    <!-- Summary metrics -->
    <fields>
        <fullName>Total_Records__c</fullName>
        <label>Total Records</label>
        <type>Number</type>
        <precision>18</precision>
        <scale>0</scale>
    </fields>
    <fields>
        <fullName>Sent_Count__c</fullName>
        <label>Sent Count</label>
        <type>Number</type>
        <precision>18</precision>
        <scale>0</scale>
    </fields>
    <fields>
        <fullName>Failed_Count__c</fullName>
        <label>Failed Count</label>
        <type>Number</type>
        <precision>18</precision>
        <scale>0</scale>
    </fields>
    <fields>
        <fullName>Template_Used__c</fullName>
        <label>Template Used</label>
        <type>Text</type>
        <length>255</length>
    </fields>
    <fields>
        <fullName>User__c</fullName>
        <label>User</label>
        <type>Lookup</type>
        <referenceTo>User</referenceTo>
        <relationshipLabel>User</relationshipLabel>
        <relationshipName>User_SMS_Logs</relationshipName>
    </fields>
    <fields>
        <fullName>Campaign__c</fullName>
        <label>Campaign</label>
        <type>Lookup</type>
        <referenceTo>Campaign</referenceTo>
        <relationshipLabel>Campaign</relationshipLabel>
        <relationshipName>Campaign_SMS_Logs</relationshipName>
    </fields>
    <fields>
        <fullName>Send_Timestamp__c</fullName>
        <label>Send Timestamp</label>
        <type>DateTime</type>
        <required>false</required>
    </fields>
</CustomObject>
